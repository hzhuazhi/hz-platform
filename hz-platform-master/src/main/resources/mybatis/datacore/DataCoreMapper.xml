    <!-- 更新基本信息 -->
    <update id="update" parameterType="com.hz.platform.master.core.model.datacore.DataCoreModel">
        UPDATE tb_hz_data_core a
        <trim prefix="set" suffixOverrides=",">
            <if test="did != null and did != '' " >
                a.did = #{did},
            </if>
            a.update_time = NOW(),
        </trim>
        <include refid="Example_Where_Clause"/>
    </update>

    <!-- 主要作用1.逻辑删除，2更新状态 -->
