    <select id="findByObject" parameterType="Object" resultType="com.hz.platform.master.core.model.datacore.DataCoreModel">
        select
        <include refid="Base_Column_List" />
        from tb_hz_data_core a
        <include refid="Example_Where_Clause"/>
