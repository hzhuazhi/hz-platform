<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hz.platform.master.core.mapper.GewaytradetypeMapper">




    <!-- 定义数据库基本列,只适用代替单表的*-->
    <sql id="Base_Column_List">
        a.id,a.geway_id,a.my_name,a.my_trade_type,a.my_service_charge,a.out_name,a.out_trade_type,a.service_charge,
        a.limit_money,a.limit_day,a.limit_month,a.remark,
        a.create_time,a.update_time,a.yn
    </sql>


    <!-- 查询条件 -->
    <sql id="Example_Where_Clause">
        where 1=1
        <trim  suffixOverrides="," >
            <if test="id != null and id != '' " >
                and a.id = #{id}
            </if>
            <if test="gewayId != null and gewayId != '' " >
                and a.geway_id = #{gewayId}
            </if>
            <if test="myName != null and myName != '' " >
                and a.my_name = #{myName}
            </if>
            <if test="myTradeType != null and myTradeType != '' " >
                and a.my_trade_type = #{myTradeType}
            </if>
            <if test="outName != null and outName != '' " >
                and a.out_name = #{outName}
            </if>
            <if test="outTradeType != null and outTradeType != '' " >
                and a.out_trade_type = #{outTradeType}
            </if>
            and a.yn = 0
        </trim>
    </sql>


    <!-- 查询条件_id -->
    <sql id="Example_Where_Clause_id">
        where 1=1
        <trim  suffixOverrides="," >
            <if test="id != null and id != '' " >
                and a.id = #{id}
            </if>
            and a.yn = 0
        </trim>
    </sql>


    <!-- 排序条件 -->
    <sql id="Example_Order_By">
        order by a.create_time asc
    </sql>

    <!-- 显示所有数据 -->
    <select id="findAll" resultType="com.hz.platform.master.core.model.geway.GewaytradetypeModel">
        select
        <include refid="Base_Column_List" />
        from tb_hz_geway_trade_type a
        <include refid="Example_Where_Clause"/>
    </select>

    <!-- 查询列表 -->
    <select id="findByCondition" parameterType="Object" resultType="com.hz.platform.master.core.model.geway.GewaytradetypeModel">
        select
        <include refid="Base_Column_List" />
        from tb_hz_geway_trade_type a
        <include refid="Example_Where_Clause"/>
    </select>

    <!-- 根据条件查询信息-->
    <select id="findByObject" parameterType="Object" resultType="com.hz.platform.master.core.model.geway.GewaytradetypeModel">
        select
        <include refid="Base_Column_List" />
        from tb_hz_geway_trade_type a
        <include refid="Example_Where_Clause"/>
        limit 1
    </select>

    <!-- 根据id查询信息-->
    <select id="findById" parameterType="java.lang.Long" resultType="com.hz.platform.master.core.model.geway.GewaytradetypeModel">
        select
        <include refid="Base_Column_List" />
        from tb_hz_geway_trade_type a
        <include refid="Example_Where_Clause_id"/>
    </select>



    <!-- 查询总数-->
    <select id="queryByCount" resultType="java.lang.Integer"  parameterType="Object">
        select count(1)
        from tb_hz_geway_trade_type a
        <include refid="Example_Where_Clause"/>
    </select>

    <!-- 查询分页列表 -->
    <select id="queryByList" parameterType="Object" resultType="com.hz.platform.master.core.model.geway.GewaytradetypeModel">
        select
        <include refid="Base_Column_List" />
        from tb_hz_geway_trade_type a
        <include refid="Example_Where_Clause"/>
        <include refid="Example_Order_By"/>
        <if test="pageNumber != null and pageNumber != ''" >
            limit #{start},#{end}
        </if>
    </select>


    <!-- 添加基本信息 -->
    <insert id="add" parameterType="com.hz.platform.master.core.model.geway.GewaytradetypeModel" >
        <!--<selectKey resultType="java.lang.Long" order="AFTER" keyProperty="id">-->
        <!--SELECT LAST_INSERT_ID() as id-->
        <!--</selectKey>-->
        insert into tb_hz_geway_trade_type
        <trim prefix="(" suffix=")" suffixOverrides="," >
            <if test="gewayName != null and gewayName != '' " >
                geway_name,
            </if>
            <if test="interfaceAds != null and interfaceAds != '' " >
                interface_ads,
            </if>
            <if test="companyName != null and companyName != '' " >
                company_name,
            </if>
            <if test="contacts != null and contacts != '' " >
                contacts,
            </if>
            <if test="phoneNum != null and phoneNum != '' " >
                phone_num,
            </if>
            <if test="payType != null and payType != '' " >
                pay_type,
            </if>
            create_time,
        </trim>
        values
        <trim prefix="(" suffix=")" suffixOverrides="," >
            <if test="gewayName != null and gewayName != '' " >
                <![CDATA[  #{gewayName}, ]]>
            </if>
            <if test="interfaceAds != null and interfaceAds != '' " >
                <![CDATA[  #{interfaceAds}, ]]>
            </if>
            <if test="companyName != null and companyName != '' " >
                <![CDATA[  #{companyName}, ]]>
            </if>
            <if test="contacts != null and contacts != '' " >
                <![CDATA[  #{contacts}, ]]>
            </if>
            <if test="phoneNum != null and phoneNum != '' " >
                <![CDATA[  #{phoneNum}, ]]>
            </if>
            <if test="payType != null and payType != '' " >
                <![CDATA[  #{payType}, ]]>
            </if>
            NOW(),
        </trim>
    </insert>

    <!-- 更新基本信息 -->
    <update id="update" parameterType="com.hz.platform.master.core.model.geway.GewaytradetypeModel">
        UPDATE tb_hz_geway_trade_type a
        <trim prefix="set" suffixOverrides=",">
            <if test="interface_adsName != null and interface_adsName != '' " >
                a.geway_name = #{interface_adsName},
            </if>
            <if test="interface_ads != null and interface_ads != '' " >
                a.interface_ads = #{interface_ads},
            </if>
            <if test="companyName != null and companyName != '' " >
                a.company_name = #{companyName},
            </if>
            <if test="contacts != null and contacts != '' " >
                a.contacts = #{contacts},
            </if>
            <if test="phoneNum != null and phoneNum != '' " >
                a.phone_num = #{phoneNum},
            </if>
            <if test="payType != null and payType != '' " >
                a.pay_type = #{payType},
            </if>
            a.update_time = NOW(),
        </trim>
        <include refid="Example_Where_Clause"/>
    </update>

    <!-- 主要作用1.逻辑删除，2更新状态 -->
    <update id="manyOperation" parameterType="Object">
        update tb_hz_geway_trade_type a
        <trim prefix="set" suffixOverrides=",">
            <if test="yn != 0 and yn >0 " >
                a.yn = #{yn},
            </if>
        </trim>
        <include refid="Example_Where_Clause_id"/>
    </update>


</mapper>